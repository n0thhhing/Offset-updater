{
  "useTabs": false,
  "tabWidth": 4,
  "requirePragma": false,
  "semi": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "singleQuote": true,
    "overrides": [
    {
      files: '*.svg',
      options: {
        parser: 'html',
      },
    },
    {
      "files": ".*rc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports"]
}